<h1>New article</h1>

<%= form_for(@article) do |form|%>
<%= form.label :title %>
<%= form.text_field :title, class: 'form-control' %>

<%= form.label :content %>
<%= form.text_field :content, class: 'form-control' %>

<br>
<%= form.submit class: 'btn btn-primary' %>
<% end %>

<p><%= link_to "Back", articles_path %></p>
